{
  "package": {
    "productName": "xuxiaowei-cloud",
    "version": "0.0.1"
  },
  "$schema": "..\\node_modules/@tauri-apps/cli\\schema.json",
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:1001",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "withGlobalTauri": false
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "cloud.xuxiaowei",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "徐晓伟工作室",
      "category": "DeveloperTool",
      "shortDescription": "徐晓伟微服务",
      "longDescription": "徐晓伟微服务 https://github.com/xuxiaowei-cloud/xuxiaowei-cloud",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "徐晓伟微服务",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
